// Sliding Window Technique
int Solution::solve(vector<int> &a, int B) {

    int n = a.size();

    int windowSize = n-B;
    int sum=0;
    for(int i=0; i<windowSize; i++) sum+=a[i];

    int minSum = sum;
    for(int i=0; i<B; i++) {
        sum-=a[i];
        sum+=a[windowSize+i];
        minSum = min(sum,minSum);
    }
    int sum1=0;
    for(int i=0; i<n; i++) sum1+=a[i];
    return sum1-minSum;
 }
